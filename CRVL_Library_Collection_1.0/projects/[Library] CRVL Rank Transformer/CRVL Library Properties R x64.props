    </Lib>
    <PostBuildEvent>
      <Command>xcopy /f /y *.h "$(projectdir)..\..\include\"
xcopy /f /y .\x64\Release\*.lib "$(projectdir)..\..\lib\x64"</Command>
    </PostBuildEvent>
  </ItemDefinitionGroup>
  <ItemGroup />
